@inproceedings{zhang2021analysis,
  author    = {Zhang, Yiqi and Deng, Xinyu and Wang, Mingyue},
  title     = {Analysis of Food Delivery Market in China: A Case Study of Meituan},
  booktitle = {Proceedings of the 2021 5th International Seminar on Education, Management and Social Sciences (ISEMSS 2021)},
  series    = {Advances in Social Science, Education and Humanities Research},
  volume    = {571},
  pages     = {261--265},
  year      = {2021},
  publisher = {Atlantis Press SARL},
  address   = {Paris, France},
  doi       = {10.2991/isemss.k.210726.052},
  url       = {https://www.atlantis-press.com/proceedings/isemss-21/125949052},
  abstract  = {以美团为案例，系统分析中国外卖市场的发展现状与前景。研究指出，在移动互联网、O2O模式及人工智能技术推动下，美团通过“消费者至上”理念、多元化业务布局（餐饮、酒店、电影票务等）及高效配送体系，占据中国外卖市场65%份额；探讨传统外卖模式向移动APP聚合平台转型趋势，提出无人机、配送机器人等技术对未来外卖市场的革新潜力，并预测2025年中国外卖APP市场收入有望达1000亿美元，为外卖企业发展提供参考。},
  note      = {会议论文，收录于Advances in Social Science, Education and Humanities Research系列出版物；基于公开数据（如美团市场份额、外卖行业收入）及学术文献，分析美团商业模式、配送方式演进（传统电话订餐→移动APP→智能配送），强调大数据与科技对行业升级的作用，遵循CC BY-NC 4.0开源许可协议}
}

@inproceedings{zhang2025influence,
  author    = {Zhang, Chen},
  title     = {The Influence Mechanism of the "Pinhaofan" Function on the Meituan Food Delivery Platform},
  booktitle = {Proceedings of the 2025 Global Conference on Economics, Business and Management (GEBM 2025)},
  series    = {Highlights in Business, Economics and Management},
  volume    = {54},
  pages     = {90--95},
  year      = {2025},
  publisher = {Atlantis Press SARL},
  address   = {Paris, France},
  doi       = {10.2991/gebm.k.250115.018},
  url       = {https://www.atlantis-press.com/proceedings/gebm-25/127894018},
  abstract  = {聚焦美团外卖“拼好饭”功能对平台发展的影响机制，通过SWOT分析指出：该功能凭借低价、免配送费等优势，2024年Q1日均订单达500万单（占平台总订单10%），显著增强平台网络效应（2023年骑手数量增19.4%至745万人）；但同时面临客单价下降、利润增长停滞的内部短板，及竞品模仿（如饿了么“拼团”）、恶性低价竞争与食品安全监管的外部威胁。研究建议美团需从短期规模导向转向长期可持续增长，强化商户监管与功能创新，为外卖行业竞争策略提供参考。},
  note      = {会议论文，收录于Highlights in Business, Economics and Management系列；基于美团官方财报（2024年Q1营收733亿元，同比增25%）、CNNIC用户数据（2024年6月外卖用户5.53亿）等实证资料，结合SWOT与VRIO分析框架，探讨“拼好饭”功能对平台用户、商户、骑手的多维度影响，明确其在O2O外卖模式创新中的定位与挑战}
}

