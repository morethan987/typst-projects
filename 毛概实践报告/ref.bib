@article{dai2023group,
  author    = {Dai, Debao and Ma, Huixian and Zhao, Min and Fan, Tijun},
  title     = {Group Buying Pricing Strategies of O2O Restaurants in Meituan Considering Service Levels},
  journal   = {Systems},
  year      = {2023},
  volume    = {11},
  number    = {12},
  pages     = {562},
  doi       = {10.3390/systems11120562},
  url       = {https://www.mdpi.com/journal/systems/article/view/systems11120562},
  abstract  = {针对后疫情时代餐饮行业与第三方平台合作开展团购服务的场景，构建包含线下零售、线上零售、线上团购三种模式的双层供应链博弈模型，分析餐厅与美团平台的最优定价及服务策略。基于1168份调研数据验证：（1）餐厅与平台合作时，最优定价策略可使双方利润最大化；（2）餐厅最优定价受消费者对服务水平的敏感度系数、服务成本系数影响；（3）餐厅利润与新增潜在消费者数量正相关。研究为餐饮企业与平台制定价格及服务策略提供参考。},
  note      = {采用Stackelberg博弈模型分析决策均衡，重点探讨平台服务水平（界面易用性、客服响应等）、餐厅服务水平（环境、 waiter服务等）对定价的影响，包含数值分析（敏感度系数、成本系数、团购成本等变量的影响）及稳健性检验，发表于MDPI旗下开源期刊，遵循CC BY 4.0许可协议}
}

